tests = ['test-pgalloc', 'test-cache', 'test-splay-tree',
         'test-splay-tree-gc', 'test-avl-tree', 'test-bitset',
         'test-vector-regression']

for prog in [smk.make_program (x, srcs = [x + '.c'], libs = [libulib])
             for x in tests]:
        smk.env.all.depends (prog)
        smk.install ('bin', prog)

